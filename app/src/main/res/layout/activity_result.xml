        <TextView
            android:text="@string/result_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textSize="20sp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/id_textView_info5"
            android:layout_toRightOf="@+id/id_textView_info5"
            android:layout_below="@+id/id_textView_results2"
            android:id="@+id/id_textView_results3" />


        <TextView
            android:text="@string/info_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="5dp"
