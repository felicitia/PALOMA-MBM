            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/id_textView_info5"
            android:layout_toRightOf="@+id/id_textView_info5"
            android:layout_below="@+id/id_textView_results4"
            android:id="@+id/id_textView_results5" />

        <TextView
            android:text="@string/info_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
