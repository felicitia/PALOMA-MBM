            android:layout_alignParentLeft="true"
            android:layout_marginTop="5dp"
            android:textSize="20sp"
            android:layout_below="@+id/id_textView_info1"
            android:id="@+id/id_textView_info2" />

        <TextView
            android:text="@string/result_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textSize="20sp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/id_textView_info5"
            android:layout_toRightOf="@+id/id_textView_info5"
            android:layout_below="@+id/id_textView_results1"
            android:id="@+id/id_textView_results2" />


        <TextView
            android:text="@string/info_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
